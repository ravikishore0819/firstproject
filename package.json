{
	"name": "olb-client",
	"version": "0.0.0",
	"scripts": {
		"start": "ng serve --proxy-config proxy-local.conf.json",
		"start:uat": "ng serve --configuration=qa",
		"start:dummy1": "json-server --watch dummy-data.json --middlewares dummy-middleware.js",
		"start:dummy2": "ng serve --proxy-config proxy-dummy.conf.json",
		"build": "ng build olb-lib && ng build --prod --deploy-url /online/ --base-href=/online/",
		"build:lib": "ng build olb-lib",
		"publish:lib": "npm publish dist/olb-lib",
		"assemble": "bestzip artifact.jar dist/olb-client/*",
		"clean": "del-cli dist/ && del-cli artifact.jar",
		"test": "ng test",
		"testci": "ng test --karma-config karma-ci.conf.js",
		"lint": "ng lint",
		"pree2e": "webdriver-manager update --proxy http://proxydr.rbfcu.local:9091 --gecko false --standalone false",
		"e2e": "ng e2e --configuration=qa --webdriver-update false"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^7.1.3",
		"@angular/cdk": "^7.1.1",
		"@angular/common": "^7.1.3",
		"@angular/compiler": "^7.1.3",
		"@angular/core": "^7.1.3",
		"@angular/forms": "^7.1.3",
		"@angular/http": "^7.1.3",
		"@angular/material": "^7.1.1",
		"@angular/platform-browser": "^7.1.3",
		"@angular/platform-browser-dynamic": "^7.1.3",
		"@angular/router": "^7.1.3",
		"@ngrx/effects": "^7.2.0",
		"@ngrx/store": "^7.2.0",
		"@ngrx/store-devtools": "^7.2.0",
		"bootstrap": "^4.1.3",
		"chart.js": "^2.7.3",
		"classlist.js": "^1.1.20150312",
		"core-js": "^2.6.0",
		"easy-pie-chart": "^2.1.7",
		"font-awesome": "^4.7.0",
		"hammerjs": "^2.0.8",
		"ng-uikit-pro-standard": "6.2.6",
		"ngx-mask": "^6.5.18",
		"ngx-swiper-wrapper": "^7.2.1",
		"rxjs": "^6.3.3",
		"rxjs-compat": "^6.3.3",
		"screenfull": "^3.3.3",
		"tachyons": "^4.11.1",
		"web-animations-js": "^2.3.1",
		"zone.js": "^0.8.26",
		"file-saver": "^2.0.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.11.0",
		"@angular-devkit/build-ng-packagr": "~0.11.0",
		"@angular/cli": "~7.1.3",
		"@angular/compiler-cli": "^7.1.3",
		"@angular/language-service": "^7.1.3",
		"@types/jasmine": "^2.8.12",
		"@types/jasminewd2": "^2.0.6",
		"@types/node": "^6.14.2",
		"bestzip": "^1.1.6",
		"codelyzer": "^4.5.0",
		"del-cli": "^1.1.0",
		"jasmine-core": "~2.8.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "^2.0.5",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "^1.4.3",
		"karma-jasmine": "^1.1.2",
		"karma-jasmine-html-reporter": "^0.2.2",
		"karma-junit-reporter": "^1.2.0",
		"ng-packagr": "^4.4.5",
		"protractor": "~5.1.2",
		"ts-node": "~4.1.0",
		"tsickle": "^0.34.0",
		"tslib": "^1.9.3",
		"tslint": "~5.9.1",
		"typescript": "^3.1.6",
		"husky": "^1.2.0",
		"lint-staged": "^8.1.0",
		"prettier": "1.15.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,js,json,scss,md}": [
			"prettier --write",
			"git add"
		]
	}
}
